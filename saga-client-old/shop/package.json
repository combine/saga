    "query-string": "^6.1.0",
    "react": "^16.4.1",
    "react-apollo": "^2.1.5",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.4.0",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.0.0-beta.6",
    "saga-server": "file:../server",
    "semantic-ui-css": "^2.3.2",
    "semantic-ui-react": "^0.82.0",
    "webpack-cli": "^3.0.8",
    "yup": "^0.24.1"
  }
}
