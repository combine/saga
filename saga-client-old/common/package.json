{
  "name": "saga-components",
  "description": "Common components for saga apps.",
  "version": "1.0.0",
  "license": "MIT",
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/combine/saga-components.git"
  },
  "engines": {
    "node": "8.x.x",
    "npm": "5.2.x"
  },
  "_moduleAliases": {
    "@components": "src/components",
    "@hocs": "src/components",
    "@containers": "src/containers",
    "@pages": "src/pages",
    "@routes": "src/routes"
  },
  "scripts": {
    "build": "bnr build",
    "pretest": "bnr pretest --path=null",
    "test": "bnr --path=null test",
    "test:all": "bnr --path=null test:all",
    "test:watch": "bnr --path=null test:watch",
    "test:all:watch": "bnr --path=null test:all:watch",
    "lint": "bnr lint",
    "postinstall": "bnr build"
  },
  "betterScripts": {
    "pretest": {
      "command": "node ./test/support/init.js",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test": {
      "command": "npx jest -c ./test/support/jest.config.js --verbose --detectOpenHandles",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test:watch": {
      "command": "npm run test -- --watch"
    },
    "lint": {
      "command": "npx eslint --ext .js,.jsx ."
    }
  },
  "devDependencies": {
    "autoprefixer": "^8.6.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "better-npm-run": "^0.1.0",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^0.28.11",
    "debug": "^3.1.0",
    "dotenv": "^5.0.1",
    "dotenv-safe": "^5.0.1",
    "dotenv-webpack": "^1.5.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "escape-string-regexp": "^1.0.5",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-react": "^7.9.1",
    "expose-loader": "^0.7.5",
    "extract-css-chunks-webpack-plugin": "^3.0.9",
    "faker": "^4.1.0",
    "file-loader": "^1.1.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.4.1",
    "json-loader": "^0.5.7",
    "node-sass": "^4.9.0",
    "prettier-eslint": "^8.8.1",
    "react-hot-loader": "^4.3.3",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^7.0.3",
    "sass-resources-loader": "^1.3.3",
    "sinon": "^4.5.0",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-client": "^4.0.3",
    "webpack-manifest-plugin": "^2.0.3",
    "webpack-merge": "^4.1.3",
    "webpack-node-externals": "^1.7.2",
    "webpack-sources": "^1.1.0",
    "yn": "^2.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "graphql": "^0.13.2",
    "lodash": "^4.17.10",
    "module-alias": "^2.1.0",
    "react": "^16.4.1",
    "react-apollo": "^2.1.5",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.0.0-beta.7",
    "semantic-ui-css": "^2.3.2",
    "semantic-ui-react": "^0.82.0"
  }
}
