{
  "name": "saga-server",
  "description": "Core framework for saga apps.",
  "version": "1.0.0",
  "license": "MIT",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/combine/saga.git"
  },
  "engines": {
    "node": "8.x.x",
    "npm": "5.2.x"
  },
  "_moduleAliases": {
    "@config": "config",
    "@init": "init",
    "@lib": "lib",
    "@middleware": "middleware",
    "@server": "server",
    "@templates": "templates"
  },
  "scripts": {
    "start": "npm run build && node test.js",
    "build": "bnr build",
    "pretest": "bnr pretest --path=null",
    "test": "bnr --path=null test",
    "test:all": "bnr --path=null test:all",
    "test:watch": "bnr --path=null test:watch",
    "test:all:watch": "bnr --path=null test:all:watch",
    "lint": "bnr lint",
    "postinstall": "npm run build"
  },
  "betterScripts": {
    "build": {
      "command": "rm -rf ./dist && babel ./src -d ./dist",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "pretest": {
      "command": "node ./test/support/init.js",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test": {
      "command": "npx jest -c ./test/support/jest.config.js --verbose --detectOpenHandles",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test:watch": {
      "command": "npm run test -- --watch"
    },
    "lint": {
      "command": "npx eslint --ext .js,.jsx ."
    }
  },
  "devDependencies": {
    "autoprefixer": "^8.6.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.0.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "better-npm-run": "^0.1.0",
    "debug": "^3.1.0",
    "dotenv": "^5.0.1",
    "eslint": "^4.19.1",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-react": "^7.10.0",
    "jest": "^23.4.1",
    "sinon": "^4.5.0"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.2.7",
    "apollo-client": "^2.3.8",
    "apollo-link-http": "^1.5.4",
    "chokidar": "^2.0.4",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "helmet": "^3.12.1",
    "lodash": "^4.17.10",
    "module-alias": "^2.1.0",
    "node-fetch": "^2.2.0",
    "react": "^16.4.2",
    "react-apollo": "^2.1.11",
    "react-dom": "^16.4.2",
    "react-loadable": "^5.5.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "webpack": "^4.16.5",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-client": "^4.1.1"
  }
}
