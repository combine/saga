{
  "name": "saga-api",
  "description": "GraphQL API for saga clients.",
  "version": "1.0.0",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/combine/saga.git"
  },
  "engines": {
    "node": "8.x.x",
    "npm": "5.2.x"
  },
  "_moduleAliases": {
    "@config": "config",
    "@db": "server/db",
    "@factories": "test/factories",
    "@graphql": "server/graphql",
    "@lib": "server/lib",
    "@middleware": "server/middleware",
    "@models": "server/models",
    "@schemas": "server/models/schemas",
    "@spec": "test/spec",
    "@support": "test/support",
    "@test": "test"
  },
  "scripts": {
    "console": "bnr console",
    "runner": "bnr runner",
    "knex": "bnr knex",
    "db:test:prepare": "bnr db:migrate",
    "db:create": "bnr db:create",
    "db:drop": "bnr db:drop",
    "db:migrate": "bnr db:migrate",
    "db:migrate:make": "bnr db:migrate:make",
    "db:rollback": "bnr db:rollback",
    "db:seed": "bnr db:seed",
    "db:fresh": "bnr db:fresh",
    "start": "bnr dev:start",
    "prod": "bnr prod:build && bnr serve",
    "serve": "bnr serve",
    "dev:start": "bnr dev:start",
    "prod:build": "bnr prod:build",
    "pretest": "bnr pretest --path=null",
    "test": "bnr --path=null test",
    "test:all": "bnr --path=null test:all",
    "test:watch": "bnr --path=null test:watch",
    "test:all:watch": "bnr --path=null test:all:watch",
    "lint": "bnr lint",
    "heroku-postbuild": "bnr prod:build"
  },
  "betterScripts": {
    "console": "node ./scripts/repl.js",
    "runner": "node ./scripts/runner.js",
    "knex": "npx babel-node $(npm bin)/knex",
    "db:create": "npx babel-node ./server/db/cli --task=create",
    "db:drop": "npx babel-node ./server/db/cli --task=drop",
    "db:migrate": "npm run knex -- migrate:latest",
    "db:migrate:make": "npm run knex -- migrate:make",
    "db:rollback": "npm run knex -- migrate:rollback",
    "db:seed": "npm run knex -- seed:run",
    "db:fresh": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed",
    "db:test:prepare": {
      "command": "npm run knex -- migrate",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "serve": {
      "command": "node ./dist/index.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "dev:start": {
      "command": "node ./server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": true
      }
    },
    "prod:build": {
      "command": "npx babel ./server -d ./dist",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "pretest": {
      "command": "node ./test/support/init.js",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test": {
      "command": "npx jest -c ./test/support/jest.config.js --verbose --detectOpenHandles",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test:watch": {
      "command": "npm run test -- --watch"
    },
    "lint": {
      "command": "npx eslint --ext .js ."
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.0.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "better-npm-run": "^0.1.0",
    "debug": "^3.1.0",
    "dotenv": "^6.0.0",
    "dotenv-safe": "^6.0.0",
    "escape-string-regexp": "^1.0.5",
    "eslint": "^5.2.0",
    "eslint-plugin-babel": "^5.1.0",
    "faker": "^4.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.4.1",
    "node-dev": "^3.1.3",
    "prettier-eslint": "^8.8.1",
    "sinon": "^6.1.4",
    "yn": "^2.0.0"
  },
  "dependencies": {
    "apollo-errors": "^1.9.0",
    "apollo-resolvers": "^1.3.1",
    "apollo-server": "^2.0.0",
    "apollo-server-express": "^2.0.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "dayjs": "^1.6.9",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "graphql-tools": "^3.0.2",
    "helmet": "^3.12.1",
    "i18n": "^0.8.3",
    "joi": "^13.4.0",
    "knex": "^0.15.2",
    "knex-db-manager": "^0.4.0",
    "lodash": "^4.17.10",
    "module-alias": "^2.1.0",
    "moment": "^2.22.2",
    "numbro": "^2.1.0",
    "objection": "^1.2.0",
    "objection-auth": "^4.0.0",
    "objection-slugify": "^3.0.1",
    "objection-unique": "^1.0.1",
    "pg": "^7.4.3",
    "pg-escape": "^0.2.0",
    "query-string": "^6.1.0",
    "uuid": "^3.2.1",
    "yup": "^0.26.0"
  }
}
